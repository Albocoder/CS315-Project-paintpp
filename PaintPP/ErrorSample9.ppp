/*This code generates 12 composite functions
and prints them
*/
var comp
var loc = Location(100,100)
var size = Size(500,500)
var boundingRect = BoundingRectangle(loc,size)
var i = 0
 
while(i< 12)
{
	arr[i] = Rect(1)
	i = i + 1
	arr[i] = boundingRect
	i = i + 1
	arr[i] = 15
	i = i + 1
	arr[i] = True
	i = i + 1
	arr[i] = Color.YELLOW
	comp = Composite(arr)
	Draw(comp,boundingRect)
}//Multiline comments are not allowed in the program, so this code would generate an error
